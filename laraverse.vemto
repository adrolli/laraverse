ckages.edit', compact('packagistPackage'));\n    }\n\n    /**\n     * Update the specified resource in storage.\n     */\n    public function update(\n        PackagistPackageUpdateRequest $request,\n        PackagistPackage $packagistPackage\n    ): RedirectResponse {\n        $this->authorize('update', $packagistPackage);\n\n        $validated = $request->validated();\n        $validated['data'] = json_decode($validated['data'], true);\n\n        $packagistPackage->update($validated);\n\n        return redirect()\n            ->route('packagist-packages.edit', $packagistPackage)\n            ->withSuccess(__('crud.common.saved'));\n    }\n\n    /**\n     * Remove the specified resource from storage.\n     */\n    public function destroy(\n        Request $request,\n        PackagistPackage $packagistPackage\n    ): RedirectResponse {\n        $this->authorize('delete', $packagistPackage);\n\n        $packagistPackage->delete();\n\n        return redirect()\n            ->route('packagist-packages.index')\n            ->withSuccess(__('crud.common.removed'));\n    }\n}\n","module":"crud:502","path":"/app/Http/Controllers/PackagistPackageController.php"},"/tests/Feature/Controllers/PackagistPackageControllerTest.php":{"deleteOnGeneration":true,"content":"<?php\n\nnamespace Tests\\Feature\\Controllers;\n\nuse App\\Models\\User;\nuse App\\Models\\PackagistPackage;\n\nuse Tests\\TestCase;\nuse Illuminate\\Foundation\\Testing\\WithFaker;\nuse Illuminate\\Foundation\\Testing\\RefreshDatabase;\n\nclass PackagistPackageControllerTest extends TestCase\n{\n    use RefreshDatabase, WithFaker;\n\n    protected function setUp(): void\n    {\n        parent::setUp();\n\n        $this->actingAs(\n            User::factory()->create(['email' => 'admin@admin.com'])\n        );\n\n        $this->seed(\\Database\\Seeders\\PermissionsSeeder::class);\n\n        $this->withoutExceptionHandling();\n    }\n\n    protected function castToJson($json)\n    {\n        if (is_array($json)) {\n